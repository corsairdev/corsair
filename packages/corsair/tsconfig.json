{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable"],
    "types": ["node"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "rootDir": "./",
    "composite": true,
    "incremental": true,
    "emitDeclarationOnly": false,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["dist", "node_modules", "sandbox/**/*"],
  "references": []
}
