    "@trpc/server": "^11.0.0",
    "@trpc/tanstack-react-query": "^11.0.0",
    "drizzle-orm": ">=0.28.0",
    "drizzle-zod": ">=0.5.0",
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesOptional": {
    "@tanstack/react-query": "*",
    "@trpc/client": "*",
    "@trpc/server": "*",
    "@trpc/tanstack-react-query": "*",
    "drizzle-orm": "*",
    "drizzle-zod": "*",
