{
  "name": "corsair",
  "version": "1.0.0",
  "description": "The TypeScript Vibe Coding SDK",
  "type": "module",
  "license": "MIT",
  "homepage": "https://corsair.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/corsairdev/corsair",
    "directory": "packages/corsair"
  },
  "keywords": [
    "typescript",
    "nextjs",
    "drizzle-orm",
    "postgres",
    "trpc",
    "tanstack-query",
    "sdk",
    "full-stack",
    "type-safe",
    "ai",
    "llm"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "watch": "tsx cli/watch/index.tsx",
    "agent-test": "tsx llm/agent/tests/agent-prompt.ts"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": {
    "corsair": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "dev-source": "./index.ts",
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./core": {
      "dev-source": "./core/index.ts",
      "types": "./dist/core/index.d.mts",
      "default": "./dist/core/index.mjs"
    },
    "./client": {
      "dev-source": "./client/index.ts",
      "types": "./dist/client/index.d.mts",
      "default": "./dist/client/index.mjs"
    },
    "./config": {
      "dev-source": "./config/index.ts",
      "types": "./dist/config/index.d.mts",
      "default": "./dist/config/index.mjs"
    },
    "./config/drizzle-postgres": {
      "dev-source": "./config/drizzle-postgres.ts",
      "types": "./dist/config/drizzle-postgres.d.mts",
      "default": "./dist/config/drizzle-postgres.mjs"
    },
    "./config/prisma-postgres": {
      "dev-source": "./config/prisma-postgres.ts",
      "types": "./dist/config/prisma-postgres.d.mts",
      "default": "./dist/config/prisma-postgres.mjs"
    },
    "./llm": {
      "dev-source": "./llm/index.ts",
      "types": "./dist/llm/index.d.mts",
      "default": "./dist/llm/index.mjs"
    },
    "./llm/agent": {
      "dev-source": "./llm/agent/index.ts",
      "types": "./dist/llm/agent/index.d.mts",
      "default": "./dist/llm/agent/index.mjs"
    },
    "./adapters": {
      "dev-source": "./adapters/index.ts",
      "types": "./dist/adapters/index.d.mts",
      "default": "./dist/adapters/index.mjs"
    },
    "./adapters/client": {
      "dev-source": "./adapters/client/index.ts",
      "types": "./dist/adapters/client/index.d.mts",
      "default": "./dist/adapters/client/index.mjs"
    },
    "./adapters/db/postgres/drizzle": {
      "dev-source": "./adapters/db/postgres/drizzle/index.ts",
      "types": "./dist/adapters/db/postgres/drizzle/index.d.mts",
      "default": "./dist/adapters/db/postgres/drizzle/index.mjs"
    },
    "./adapters/db/postgres/prisma": {
      "dev-source": "./adapters/db/postgres/prisma/index.ts",
      "types": "./dist/adapters/db/postgres/prisma/index.d.mts",
      "default": "./dist/adapters/db/postgres/prisma/index.mjs"
    },
    "./plugins": {
      "dev-source": "./plugins/index.ts",
      "types": "./dist/plugins/index.d.mts",
      "default": "./dist/plugins/index.mjs"
    },
    "./plugins/slack": {
      "dev-source": "./plugins/slack/index.ts",
      "types": "./dist/plugins/slack/index.d.mts",
      "default": "./dist/plugins/slack/index.mjs"
    },
    "./cli": {
      "dev-source": "./cli/index.ts",
      "types": "./dist/cli/index.d.mts",
      "default": "./dist/cli/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts"
      ],
      "core": [
        "./dist/core/index.d.mts"
      ],
      "client": [
        "./dist/client/index.d.mts"
      ],
      "config": [
        "./dist/config/index.d.mts"
      ],
      "config/drizzle-postgres": [
        "./dist/config/drizzle-postgres.d.mts"
      ],
      "config/prisma-postgres": [
        "./dist/config/prisma-postgres.d.mts"
      ],
      "llm": [
        "./dist/llm/index.d.mts"
      ],
      "llm/agent": [
        "./dist/llm/agent/index.d.mts"
      ],
      "adapters": [
        "./dist/adapters/index.d.mts"
      ],
      "adapters/client": [
        "./dist/adapters/client/index.d.mts"
      ],
      "adapters/db/postgres/drizzle": [
        "./dist/adapters/db/postgres/drizzle/index.d.mts"
      ],
      "adapters/db/postgres/prisma": [
        "./dist/adapters/db/postgres/prisma/index.d.mts"
      ],
      "plugins": [
        "./dist/plugins/index.d.mts"
      ],
      "plugins/slack": [
        "./dist/plugins/slack/index.d.mts"
      ],
      "cli": [
        "./dist/cli/index.d.mts"
      ]
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.68",
    "dotenv": "^16.4.5",
    "openai": "^4.103.0",
    "pg": "^8.16.3",
    "superjson": "^2.2.3",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
